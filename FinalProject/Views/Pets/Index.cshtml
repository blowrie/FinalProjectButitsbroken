                @Html.DisplayFor(modelItem => item.Vaccination.Type)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Visit.ClinicName)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Weight.WeightID)
            </td>
            <td>
                @Html.ActionLink("Edit", "Edit", new { id = item.PetID }) |
